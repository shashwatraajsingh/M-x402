{
  "name": "monad-x402",
  "version": "1.0.0",
  "description": "HTTP 402 Payment Protocol SDK for Monad - Axios-compatible payment wrapper with automatic network detection and zero fallbacks",
  "author": "Shashwat Raj Singh",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/shashwatraajsingh/MonX402"
  },
  "homepage": "https://github.com/shashwatraajsingh/MonX402",
  "keywords": [
    "x402",
    "monad",
    "payment",
    "micropayment",
    "http-402",
    "blockchain",
    "cryptocurrency",
    "ethereum",
    "evm",
    "axios",
    "axios-compatible",
    "typescript",
    "nextjs",
    "middleware",
    "ai-payments",
    "machine-to-machine"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./server": {
      "types": "./dist/server/index.d.ts",
      "default": "./dist/server/index.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "default": "./dist/types/index.js"
    }
  },
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "npm run build:sdk && npm run build:demo",
    "build:sdk": "tsc -p tsconfig.sdk.json",
    "build:demo": "next build --turbopack",
    "start": "next start",
    "demo": "tsx scripts/demo-agent.ts",
    "prepublishOnly": "npm run build:sdk",
    "test": "echo \"Tests coming soon\" && exit 0"
  },
  "peerDependencies": {
    "next": ">=15.0.0"
  },
  "dependencies": {
    "@icons-pack/react-simple-icons": "^13.8.0",
    "@langchain/core": "^0.3.0",
    "@langchain/google-genai": "^0.1.0",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-use-controllable-state": "^1.2.2",
    "@shikijs/transformers": "^3.14.0",
    "@vercel/analytics": "^1.5.0",
    "axios": "^1.12.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "ethers": "^6.15.0",
    "langchain": "^0.3.0",
    "lucide-react": "^0.548.0",
    "next": ">=15.0.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "shiki": "^3.14.0",
    "tailwind-merge": "^3.3.1",
    "x402-axios": "^0.6.6",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "tailwindcss": "^4",
    "tsx": "^4.7.1",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5"
  }
}
